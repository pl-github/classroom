<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="code_user.security.core.user.fos_user_provider.class">Code\UserBundle\Security\Core\User\FOSUBUserProvider</parameter>
        <parameter key="code_user.provider.github.properties" type="collection">
            <parameter key="github">githubId</parameter>
        </parameter>
    </parameters>

    <services>
        <service id="code_user.security.core.user.fos_user_provider" class="%code_user.security.core.user.fos_user_provider.class%">
            <argument type="service" id="fos_user.user_manager" />
            <argument>%code_user.provider.github.properties%</argument>
        </service>
    </services>

</container>
